
 <h3>Product Details</h3>

 <style>
  table, th, td {
      border: 1px solid black;
      border-collapse: collapse;
      text-align: center;
  }
  </style>


 <table>
  <% data.forEach(function(dbData){ %>
    <tr>
      <td> <%= dbData.name %> </td>
      <td> <%= dbData.description %> </td>
      <td> <%= dbData.price %> </td>
      <td> <%= dbData.availability %> </td>
      
    </tr>
<% }); %>
</table>